Signed-off-by: Ayush Singh <ayush@beagleboard.org>
---
 zola.metainfo.xml | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/zola.metainfo.xml b/zola.metainfo.xml
index ddf1d94a..d7afddaf 100644
--- a/zola.metainfo.xml
+++ b/zola.metainfo.xml
@@ -5,7 +5,7 @@
 
   <name>Zola</name>
   <summary>A fast static site generator in a single binary with everything built-in</summary>
-  <developer>
+  <developer id="org.getzola">
     <name>Vincent Prouillet and contributors</name>
   </developer>
 
